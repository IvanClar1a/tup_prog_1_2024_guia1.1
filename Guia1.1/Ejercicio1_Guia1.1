Proceso sin_titulo
	
	Definir cantMl, cantLitros Como Real;
	
	Escribir "Ingresar los mililitros";
	Leer  cantMl;
	
	cantLitros <- (cantMl * 1 ) / 1000;
	
	Definir quimicoA, quimicoB Como Real;
	
	quimicoA <- ( cantLitros * 2.55) / 1;
	
	quimicoB <- ( cantLitros * 5.78) / 1;
	
	Escribir  " Total de gramos a utilizar del quimico A es de: ", quimicoA, " gramos";
	Escribir  " Total de gramos a utilizar del quimico b es de: ", quimicoB, " gramos";
FinProceso
